<h1>Listing courses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.name %></td>
        <td><%= course.description%></td>
        <td><%= course.timeline %></td>
        <td><%= course.instructors %></td> 
      </tr>
    <% end %>
  </tbody>
</table>
