   <div class="form-group">
    <%= f.label :name, class: "control-label col-md-4" %>
    <div class="col-md-8">
      <%= f.input_field :name, class: "text-field form-control", html: {spellcheck: "false"} %>
    </div>
  </div>
  <div>
  	<%= f.label :description, class: "control-label col-md-4" %>
    <div class="col-md-8">
      <%= f.input_field :description, class: "text-area form-control", html: {spellcheck: "true"} %>
    </div>
  </div>
  <div class="input-daterange" data-behaviour="datepicker" id="datepicker" data-date-format="dd/mm/yyyy">
  	<%= f.label :from, class: "control-label col-md-4" %>
    <div class="col-md-8">
      <%= f.date_field :timeline, class: "text-field form-control timeline","data-provide" => 'datepicker' %>
    </div>
    <%= f.label :to, class: "control-label col-md-4" %>
    <div class="col-md-8">
      <%= f.date_field :timeline_to, class: "text-field form-control timeline","data-provide" => 'datepicker' %>
    </div>     	
  </div>
  <div>
  	<%= f.label :instructors, class: "control-label col-md-4" %>
    <div class="col-md-8">
      <%= f.input_field :instructors, class: "text-field form-control", html: {spellcheck: "false"} %>
    </div>    	
  </div>
  <div>
    <%= f.label :image, class: "control-label col-md-4" %>
    <div class="field col-md-8">
      <%= f.file_field :image %>
    </div>
  </div>    
  <div class="form-actions">
    <%= f.button :submit %>
  </div>